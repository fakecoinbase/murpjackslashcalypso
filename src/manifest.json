{
  "manifest_version": 2,
  "name": "Calypso",
  "version": "1.1.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "extension.html",
    "default_icon": "images/logo-favicon-tablet.png"
  },
  "permissions": ["activeTab", "storage", "tabs"]
}
